{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:vue/vue3-recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "vue/no-multiple-template-root": "off",
    "no-undef": "off",
    "no-unused-vars": "warn",
    "vue/no-v-model-argument": "warn",
    "vue/no-v-html": "off"
  }
}
