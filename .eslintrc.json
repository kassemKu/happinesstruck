{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["prettier", "eslint:recommended", "plugin:vue/essential"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["vue"],
    "rules": {
        "vue/no-multiple-template-root": "off"
    }
}
